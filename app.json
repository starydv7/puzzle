{
  "expo": {
    "name": "Puzzle Fun for Kids",
    "slug": "puzzle-fun-kids",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4A90E2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.puzzlefun.kids"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4A90E2"
      },
      "package": "com.puzzlefun.kids",
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "privacy": "public"
  }
}

